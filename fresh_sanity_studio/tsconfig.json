{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "..",
    "allowImportingTsExtensions": true,
    "paths": {
      "@/*": ["./*"],
      "@lib/*": ["lib/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["fresh_sanity_studio/utils/*"],
      "@schemas/*": ["fresh_sanity_studio/schemas/*"],
      "@plugins/*": ["fresh_sanity_studio/plugins/*"]
    },
    "typeRoots": [
      "../node_modules/@types",
      "./types",
      "../types"
    ],
    "strictNullChecks": true
  },
  "include": [
    "lib/**/*.ts",
    "lib/**/*.tsx",
    "fresh_sanity_studio/components/**/*.ts",
    "fresh_sanity_studio/components/**/*.tsx",
    "fresh_sanity_studio/plugins/**/*.ts",
    "fresh_sanity_studio/plugins/**/*.tsx",
    "fresh_sanity_studio/schemas/**/*.ts",
    "fresh_sanity_studio/schemas/**/*.tsx",
    "fresh_sanity_studio/lib/**/*.ts",
    "fresh_sanity_studio/lib/**/*.tsx",
    "fresh_sanity_studio/scripts/**/*.ts",
    "fresh_sanity_studio/scripts/**/*.mts",
    "fresh_sanity_studio/deskStructure.tsx",
    "fresh_sanity_studio/sanity.config.tsx",
    "fresh_sanity_studio/sanity.cli.ts",
    "fresh_sanity_studio/types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".sanity",
    "**/*.js",
    "**/*.jsx"
  ]
}
