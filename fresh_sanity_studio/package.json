{
  "name": "fresh",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "dotenv -e .env -- cross-env NODE_ENV=development NEXT_PUBLIC_ENVIRONMENT=development sanity dev",
    "dev:staging": "dotenv -e .env -- cross-env NODE_ENV=development NEXT_PUBLIC_ENVIRONMENT=staging SANITY_STUDIO_DATASET=staging sanity dev",
    "start": "dotenv -e .env -- cross-env NODE_ENV=production SANITY_STUDIO_DATASET=production sanity start",
    "build": "NODE_ENV=production SANITY_STUDIO_DATASET=production sanity build",
    "deploy": "dotenv -e ../.env -- cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=production sanity deploy --token=$SANITY_STUDIO_API_TOKEN --no-verify",
    "deploy:staging": "dotenv -e .env -- cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=staging SANITY_STUDIO_DATASET=staging sanity deploy --token=$SANITY_AUTH_TOKEN --no-verify",
    "deploy-graphql": "sanity graphql deploy",
    "create-singletons": "node --loader ts-node/esm scripts/createSingletons.mts",
    "create-singletons:staging": "SANITY_STUDIO_DATASET=staging node --loader ts-node/esm scripts/createSingletons.mts",
    "test-connection": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node lib/testConnection.ts",
    "deploy:with-env": "dotenv -e .env -- sanity deploy --token=$SANITY_AUTH_TOKEN --no-verify"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/color-input": "^4.0.1",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-kit": "^5.1.18",
    "@sanity/structure": "^2.36.2",
    "@sanity/types": "^3.62.3",
    "@sanity/ui": "^2.8.18",
    "@sanity/vision": "^3.62.3",
    "@types/uuid": "^10.0.0",
    "get-youtube-id": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "sanity": "^3.62.3",
    "sanity-plugin-hotspot-array": "^2.1.0",
    "sanity-plugin-iframe-pane": "^3.1.6",
    "sanity-plugin-media": "^2.3.2",
    "styled-components": "^6.1.13",
    "uuid": "^11.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1"
  },
  "devDependencies": {
    "@sanity/cli": "^3.64.2",
    "@sanity/client": "^6.22.4",
    "@types/node": "^22.8.7",
    "@types/react": "^18.0.25",
    "@types/styled-components": "^5.1.34",
    "cross-env": "^7.0.3",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "tailwindcss": "^3.4.14",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "type": "module"
}
