{
  "name": "stak_app",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "fresh_sanity_studio"
  ],
  "scripts": {
    "clean": "rimraf .next fresh_sanity_studio/dist .netlify/cache",
    "dev": "cross-env NODE_ENV=development next dev",
    "dev:staging": "cross-env NODE_ENV=development NEXT_PUBLIC_ENVIRONMENT=staging SANITY_STUDIO_DATASET=staging next dev",
    "build": "cross-env NODE_ENV=production npm install tailwindcss && npm run fresh-studio:build && next build",
    "start": "next start",
    "lint": "next lint",
    "build:staging": "cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=staging SANITY_STUDIO_DATASET=staging next build",
    "start:staging": "cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=staging SANITY_STUDIO_DATASET=staging next start",
    "build:production": "cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=production SANITY_STUDIO_DATASET=production NEXT_PUBLIC_SANITY_DATASET=production DISABLE_ESLINT_PLUGIN=true next build",
    "start:production": "cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=production SANITY_STUDIO_DATASET=production next start",
    "fresh-studio:dev": "cd fresh_sanity_studio && cross-env NODE_ENV=development npm run dev",
    "fresh-studio:build": "cd fresh_sanity_studio && npm ci && npm run build && cd ..",
    "fresh-studio:deploy": "cd fresh_sanity_studio && npm run deploy:with-env",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "dev:all:staging": "concurrently \"npm run dev:staging\" \"npm run fresh-studio:dev:staging\"",
    "fresh-studio:dev:staging": "cd fresh_sanity_studio && SANITY_STUDIO_DATASET=staging npm run dev:staging",
    "fresh-studio:start": "cd fresh_sanity_studio && cross-env NODE_ENV=production NEXT_PUBLIC_ENVIRONMENT=production SANITY_STUDIO_DATASET=production npm run start",
    "prod": "npm run build:production && npm run start:production",
    "deploy": "cd fresh_sanity_studio && cross-env SANITY_STUDIO_API_TOKEN=$SANITY_STUDIO_API_TOKEN sanity deploy --no-verify",
    "deploy:staging": "cd fresh_sanity_studio && cross-env SANITY_STUDIO_DATASET=staging SANITY_AUTH_TOKEN=$SANITY_AUTH_TOKEN sanity deploy --no-verify",
    "deploy:production": "cd fresh_sanity_studio && cross-env SANITY_STUDIO_DATASET=production SANITY_AUTH_TOKEN=$SANITY_AUTH_TOKEN sanity deploy --no-verify",
    "deploy-graphql": "sanity graphql deploy",
    "create-singletons": "node --loader ts-node/esm scripts/createSingletons.mts",
    "create-singletons:staging": "SANITY_STUDIO_DATASET=staging node --loader ts-node/esm scripts/createSingletons.mts",
    "test-connection": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node lib/testConnection.ts",
    "deploy:with-env": "SANITY_STUDIO_API_TOKEN=${SANITY_STUDIO_API_TOKEN} sanity deploy --no-verify",
    "dev:env": "dotenv -e .env -- cross-env SANITY_STUDIO_API_TOKEN=$SANITY_AUTH_TOKEN NODE_ENV=development next dev",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules'), 'node_modules', 'dir') } catch (e) {}\""
  },
  "dependencies": {
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@jridgewell/gen-mapping": "^0.3.5",
    "@netlify/functions": "^2.8.2",
    "@portabletext/react": "^3.1.0",
    "@sanity/client": "^6.22.1",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/preview-kit": "^5.1.18",
    "@sanity/types": "^3.62.3",
    "@sanity/ui": "^2.8.13",
    "@sanity/vision": "^3.62.3",
    "@sanity/visual-editing": "^2.4.2",
    "@stripe/stripe-js": "^4.7.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "animejs": "^3.2.1",
    "axios": "^1.7.7",
    "canvas-confetti": "^1.5.1",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "dompurify": "^3.1.7",
    "dotenv": "^16.4.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^11.11.17",
    "get-audio-duration": "^4.0.1",
    "get-youtube-id": "^1.0.1",
    "glob": "^11.0.0",
    "headroom.js": "^0.12.0",
    "howler": "^2.2.4",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lru-cache": "^11.0.1",
    "next": "^15.0.1",
    "next-sanity": "^9.8.8",
    "node-fetch": "^2.7.0",
    "p5": "^1.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^5.3.0",
    "react-intersection-observer": "^9.13.1",
    "react-lazy-load-image-component": "^1.6.2",
    "react-lazyload": "^3.2.1",
    "react-player": "^2.16.0",
    "react-youtube": "^10.1.0",
    "rimraf": "^6.0.1",
    "sanity": "^3.62.3",
    "sanity-plugin-hotspot-array": "^2.1.0",
    "sanity-plugin-iframe-pane": "^3.1.6",
    "sanity-plugin-media": "^2.3.2",
    "stripe": "^17.1.0",
    "styled-components": "^6.1.13",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.7",
    "@netlify/plugin-nextjs": "^5.8.1",
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/animejs": "^3.1.12",
    "@types/canvas-confetti": "^1.6.4",
    "@types/howler": "^2.2.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.34",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
  }
}